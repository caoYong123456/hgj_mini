
.margin-top-20{
	margin-top: 20rpx;
	position: relative;
	top: 0;
  }
  
  .head{
	padding: 40rpx 20rpx 20rpx 40rpx;
	display: flex;
	justify-content: space-between;
	color: #fff;
  }
  .rq-name{
	font-size: 32rpx;
  }
  .rq-door{
	font-size: 28rpx;
	padding-top: 10rpx;
  }
  
  .visit-info-bg{
	position: absolute;
	top:0;
	left:0;
	width:100%;
	height:240rpx;
	z-index:-1;
	border-bottom-right-radius:2em 0.15em;
	border-bottom-left-radius:2em 0.15em;
  }
  
  .visit-log-view{
	display: flex;
	display: -webkit-flex;
	justify-content: center;
	align-items:center;
	/* padding:  */
  }
  .visit-log-btn{
	font-size:24rpx;
	border:1px solid #fff;
	padding:4rpx 16rpx;
	border-radius: 0.3em;
  }
  .visit-log-tab{
	padding-top:40rpx;
  }
  .visit-passcode-view{
	background-color:#fff;
	border-radius:0em 0em 0.5em 0.5em;
  }
  .visit-quickcode-view{
	background-color:#fff;
	text-align:center;
	padding-top:40rpx;
  }
  .visit-quickcode-label{
	margin-top:60rpx;
	font-size:48rpx;
  }
  .input-class{
	width: 60%;
	border: 1px solid #d0d0d0;
	border-radius: 0.25em;
	text-align: left;
	height: 60rpx !important;
	line-height: 60rpx !important;
  }
  .van-cell-custon-class{
	padding-bottom:0px !important;
  }
  .van-cell-input-class{
	padding-top:10rpx !important;
  }
  .tab-class{
	background-color: #EEEEEE;
	border-radius:0.5em;
	color: #1B1B1B;
	font-size: 32rpx !important;
	border: none !important;
  }
  .tab-active-class{
	border: none !important;
	font-size: 32rpx !important;
	background-color: #fff !important;
	color: #189AFE !important;
  }
  .van-tabs--card .van-tabs__wrap{
	height: 80rpx !important;
	line-height: 80rpx !important;
	border:none !important;
  }
  .van-tab {
	height: 80rpx !important;
	line-height: 80rpx !important;
	border:none !important;
  }
  .nav-class{
	border: none !important;
	background-color: #fff;
	height: 80rpx !important;
	line-height: 80rpx !important;
	font-size: 32rpx !important;
	border-radius: 0.5em 0.5em 0em 0em  !important;
  }
  .vant-radio1-class{
	margin-top:10rpx !important;
  }
  .vant-radio2-class{
	margin-top:30rpx !important;
  }
  .custom-class{
	padding-left: 30rpx;
	padding-right: 30rpx;
	border-radius: 0.5em !important;
  }
  .vant-btn-class{
	width: 60%;
	height: 66rpx !important;
	border-radius: 0.5em !important;
  }
  .van-cell-title-class{
	flex:.3 !important;
  }
  .create-passcode-btn{
	text-align:center;
	margin-top:20rpx;
	padding-bottom: 20rpx;
  }
  .vant-cell-value-class{
	text-align: left !important;
  }
  .van-tabs__scroll--card{
	border-radius: 0.5em !important;
  }
  
  .weui-search-bar__box {
	position: relative;
	padding-left: 0px;
	padding-right: 32px;
	width: 60%;
	border: 1px solid #d0d0d0;
	box-sizing: border-box;
	text-align: left;
	z-index: 1;
	border-radius: 0.25em;
  }
  
  .weui-search-bar__input {
	margin-left:4rpx;
  }
  
  .weui-icon-clear {
	position: absolute;
	top: 18rpx;
	right: 0;
	bottom: 0;
	padding: 0 12rpx;
	font-size: 0;
  }
  

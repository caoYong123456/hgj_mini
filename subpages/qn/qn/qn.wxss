page{
  border-top: 2rpx solid #F2F2F2;
  font-family: Microsoft YaHei Regular, Microsoft YaHei Regular-Regular;
  font-size: 32rpx;
  background-color: #FFFFFF;
}

.qn-bg{
  width: 100%;
  height: 282rpx;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}
.tab{
  padding: 54rpx 26rpx 0 54rpx;
}
.tab-col{
  text-align: center;
  font-size: 32rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  line-height: 44rpx;
  color: #363f4d;
}
.tab-selected{
  color: #179AFD;
}
.tab-selected::after{
  content: "";
  display: block;
  margin-left: 82rpx;
  width:60rpx;
  height:16rpx;
  border-bottom:6rpx solid #179AFD;
  border-radius: 3rpx;
}

.qn-view{
  padding: 0 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  line-height: 40rpx;
}
.qb-row{
  padding: 36rpx 0;
  border-bottom: solid 1px #f2f2f2;
}
.qn-icon{
  width: 32rpx;
  height: 32rpx;
  margin-top: 5rpx;
}
.qb-first{
  padding-left: 20rpx;
}

page{
  height: 100%;
}

.visit-log-item{
  border-radius:0.4em;
  margin-top:50rpx 20rpx;
  background-color:#fff;

}

.visit-log-name{
  flex: 5;
  text-align: left;
  color: #666666;
  font-size: 28rpx;
}

.visit-log-title{
  display: flex;
  margin:20rpx 10rpx 0px 10px;
  border-bottom: 2px dashed #D5D5D5;
  padding: 10rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}

.visit-log-stat{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-end;
  font-size: 28rpx;
  color: #1798dc;
  flex: 5;
  text-align: right;
  margin-right: 20rpx;
}

.loading-container {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  font-size: 22rpx;
  color: #666666;
  margin-top: 10rpx;
  padding-bottom: 20rpx;
}

.check-visit-code-view{
  text-align:right;
  padding-top: 10rpx;
  padding-right:20rpx;
}

.blue_color{
  color: #189AFE;
}

.green_color{
  color:#14D164;
}

.grey_color{
  color: #DCDCDC;
}

.van-cell{
  padding:10rpx 30rpx !important;
}

.vant-cell-title-class{
  color: #666666;
}

.check-visitlog-btn{
  border-radius: .3em !important;
}

.close-visitlog-btn{
  background-color: #fff !important;
	border-radius: .3em !important;
	color: #ABABAB !important;
  border: 1px solid #ABABAB !important;
  margin-left: 20rpx !important;
}

.empty-view{
  height: 100%;
  background-color: #fff;
}

.qn-title{
  margin:20rpx 10rpx 0px 10px;
  border-bottom: 1px dashed #D5D5D5;
  padding: 10rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  color: #666666;
  text-decoration: underline;
  text-decoration-color: rgb(80, 163, 231);
}

.qn-title-len{
  font-size: 16px;
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:1;
  text-decoration: underline;
  text-decoration-color: rgb(109, 183, 252);
}

.last_font{
  margin-top: 50rpx;
  font-size: 24rpx;
  color: #C6C6C6;
  line-height: 42rpx;
  text-align: center;
}

.date_font{
  font-size: 24rpx;
  color: #C6C6C6;
  line-height: 40rpx;
  text-align: center;
}













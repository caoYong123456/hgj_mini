page{
  border-top: 2rpx solid #F2F2F2;
  font-size: 32rpx;
  background-color:#FFFFFF;
}

.page-container{
  margin: 16rpx 0rpx 0rpx 0rpx;
  /* background-color:#FFFFFF; */
}

.no-data-container{
  height: 100%;
  background-color: #fff;
}

.qnName-view{
  text-align: center;
  padding: 12rpx 54rpx;
  font-size: 36rpx;
  color: #363f4d;
  font-weight: 500;
  line-height: 74rpx;
  word-break:break-all;
}

.qnDesc-view{
  text-align: left;
  padding: 16rpx 32rpx;
  margin-bottom: 32rpx;
  background-color: #EAEAEA;
  font-weight: 400;
  line-height: 56rpx;
  /* font-size: 32rpx; */
  color: #666666;
  word-break:break-all;
}

.queDesc-mb{
  margin-bottom:40rpx;
  font-size: 32rpx;
  font-family: PingFangSC, PingFangSC-Medium;
  font-weight: 500;
  color: #363f4d;
  line-height: 44rpx;
}

.queDesc{
  /* font-size: 32rpx; */
  font-weight: 400;
  text-align: left;
  color: #666666;
  line-height: 44rpx;
}

.queClass{
  margin-bottom: 30rpx;
  padding: 0 32rpx 16rpx 32rpx;
}

.optDesc{
  /* font-size: 32rpx; */
  font-weight: 400;
  text-align: left;
  color: #777777;
  line-height: 60rpx;
  width: 65%;
}
.statistics_cls{
  font-weight: 400;
  color: #c8c9cc;
  font-size: 28rpx;
}
.sign-area{
  padding: 10rpx 40rpx;
  text-align: center;
}

.optClass{
  margin-top: 12rpx;
  padding: 20rpx;
  /* border:2rpx solid #F0F4F7; */
  border-radius: 4rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.cellgroup{
  border:2rpx solid #F0F4F7;
  border-radius: 4rpx;
}
.sign-img-area{
  height: 112rpx;
  background-color: #f6f6f6;
  border-radius: 6px;
  font-size: 28rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  color: #999999;
  line-height: 40rpx;
  align-items: center;
  display: flex;
  justify-content: center;
  padding-left: 60rpx;
}
.sign-img-title{
  font-size: 32rpx;
  font-family: PingFangSC, PingFangSC-Medium;
  font-weight: 500;
  color: #363f4d;
  line-height: 100rpx;
}
.sign-img{
  -webkit-transform: rotate(270deg);
  -moz-transform: rotate(270deg);
  -o-transform: rotate(270deg);
  -ms-transform: rotate(270deg);
  transform: rotate(270deg);
  height: 350rpx;
  width: 100rpx;
  margin: -20rpx;
}
.sign-house{
  font-size: 32rpx;
  font-family: PingFangSC, PingFangSC-Medium;
  font-weight: 500;
  text-align: left;
  color: #363f4d;
  line-height: 56rpx;
}

.van-radio__label--disabled,.van-checkbox__label--disabled {
  color: #666666!important;
  color: var(--radio-disabled-label-color,#666666)!important;
  font-size: 28rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  line-height: 40rpx!important;
}
.van-radio__icon--disabled {
  background-color: white!important;
  background-color: var(--radio-disabled-background-color,white);
}
.van-radio__icon--disabled.van-radio__icon--checked {
  color: #368DFF!important;
  color: var(--radio-disabled-icon-color,#368DFF)!important;
  border-color: #368DFF!important;
  border-color: var(--radio-disabled-icon-color,#368DFF)!important;
}
.van-checkbox__icon--disabled {
  background-color: white!important;
  background-color: var(--checkbox-disabled-background-color,white)!important;
}

.van-checkbox__icon--disabled.van-checkbox__icon--checked {
  color: #368DFF!important;
  color: var(--checkbox-disabled-icon-color,#368DFF)!important;
  border-color: var(--checkbox-disabled-icon-color,#368DFF)!important;
}

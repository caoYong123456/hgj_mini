page{
  border-top: 2rpx solid #F2F2F2;
  font-family: Microsoft YaHei Regular, Microsoft YaHei Regular-Regular;
  font-size: 32rpx;
  background-color: #FFFFFF;
}

.page-container{
  margin: 24rpx 0;
  height: 100%;
}

.page-container2{
  margin: 0 24rpx;
}

.no-data-container{
  padding-top: 128rpx;
  text-align: center;
  font-size: 28rpx;
  font-weight: 400;
  color: #ABABAB;
}

.qnTitle-view{
  text-align: left;
  padding-left: 16rpx;
  margin-top: 38rpx;
  margin-bottom: 26rpx;
  line-height: 48rpx;
}
.qnTitle-text{
  color: #474747;
  font-weight: bold;
}
.exp_qnTitle-text{
  color: #ABABAB;
  font-weight: bold;
}

.qnInfoC{
  padding: 2rpx 24rpx;
  border: 0.2rpx solid #F2F2F2;
  margin-bottom: 10rpx;
  border-radius: 7rpx;
  background-color:#FFFFFF;
}

.qnNameC .van-row{
  /* font-size: 32rpx; */
  font-weight: 400;
  text-align: left;
  color: #474747;
  line-height: 88rpx;
  /* border-bottom: 2rpx dashed #F2F2F2; */
}

.dateTimeC .van-row{
  /* font-size: 32rpx; */
  font-weight: 400;
  text-align: left;
  color: #ABABAB;
  line-height: 88rpx;
}

.loading-container {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  color: #666666;
  margin-top: 6rpx;
  padding-bottom: 20rpx;
}

.modify-container {
  /* display: flex; */
  flex-direction: row;
  justify-content: flex-end;
}

.button {
  width: 96rpx;
  height: 46rpx;
  line-height: 48rpx;
  border:0.6rpx solid #169AFF;
  border-radius: 10rpx;
  color: #169AFF;
  background-color: white;
  text-align: center;
  /* font-size: 32rpx; */
}

.qnNameText{
  text-overflow:ellipsis;
  z-index: -1;
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}



/* **************************************************************************************************************** */
.qn-bg{
  width: 100%;
  height: 282rpx;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}
.tab{
  padding: 54rpx 26rpx 0 54rpx;
}
.tab-col{
  text-align: center;
  font-size: 32rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  line-height: 44rpx;
  color: #363f4d;
}
.tab-selected{
  color: #179AFD;
}
.tab-selected::after{
  content: "";
  display: block;
  margin-left: 82rpx;
  width:60rpx;
  height:16rpx;
  border-bottom:6rpx solid #179AFD;
  border-radius: 3rpx;
}

.qn-view{
  padding: 0 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  line-height: 40rpx;
}
.qb-row{
  padding: 36rpx 0;
  border-bottom: solid 1px #f2f2f2;
}
.qn-icon{
  width: 32rpx;
  height: 32rpx;
  margin-top: 5rpx;
}
.qb-first{
  padding-left: 20rpx;
}








